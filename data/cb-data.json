    "recipes": [
      {
        "title": "Bean Salsa",
        "details": [
          {
            "photo": "",
            "haiku": [
              {
                "1": "Zingy, sweet and tart",
                "2": "A great salsa for dipping",
                "3": "This doesn't last long"
              }
            ],
            "ingredients": [
              {
                "1": "1 15-ounce can of Black Beans (rinsed and drained)",
                "2": "1 15-ounce can of Garbanzo Beans (rinsed and drained)",
                "3": "1 12-ounce can of Corn (rinsed and drained)",
                "4": "1/4 cup chopped Onion",
                "5": "1/4 cup 7Chopped Cilantro",
                "6": "1/4 cup Lime Juice",
                "7": "1 teaspoon Jalapeno",
                "8": "2 1/4 teaspoons Cumin",
                "9": "3 Tablespoons Sugar"
              }
            ],
            "directions": "Combine ingredients, stir and chill for a couple hours.  Even better the next day."
          }
        ]
